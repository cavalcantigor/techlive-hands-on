    "apollo-server-cache-redis": "^1.2.1",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "mongoose": "^5.9.16",
    "typescript": "^3.9.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/config": "0.0.36",
    "@types/jest": "^25.2.3",
    "@types/mongodb": "^3.5.20",
    "@types/mongoose": "^5.7.21",
    "@types/sinon": "^9.0.4",
    "apollo-server-testing": "^2.14.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "sinon": "^9.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2"
  }
